{
  "name": "gp-boilerplate",
  "version": "4.0.0",
  "description": "Grabarz & Partner - Frontend Boilerplate",
  "main": "none",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "heroku": "NODE_ENV=production node node_modules/gulp/bin/gulp run --serverConfig=./env/config/server.json --gulpFileMap=./env/config/gulp-file-map.json --server=gp-boilerplate-webserver",
    "build": "NODE_ENV=build node node_modules/gulp/bin/gulp build --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "dev": "NODE_ENV=development node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=gp-boilerplate-webserver",
    "dev-ip": "NODE_ENV=development node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=gp-boilerplate-webserver --ip=true",
    "prod": "NODE_ENV=production node node_modules/gulp/bin/gulp run --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=gp-boilerplate-webserver",
    "server": "NODE_ENV=development node node_modules/gulp/bin/gulp default --serverConfig=/env/config/hapi/config --server=gp-boilerplate-webserver",
    "test": "npm run build",
    "template": "yo gp-boilerplate:hbs-template",
    "template-package": "yo gp-boilerplate:hbs-package-template",
    "create-package": "yo gp-boilerplate:package",
    "image-opt": "yo gp-boilerplate:image-opt",
    "svg-opt": "yo gp-boilerplate:svg-opt",
    "test-task-grid": "NODE_ENV=build node node_modules/gulp/bin/gulp grid --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "test-task-svg-symbols": "NODE_ENV=build node node_modules/gulp/bin/gulp svg-symbols --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "test-task-svgo": "NODE_ENV=build node node_modules/gulp/bin/gulp svgo --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/",
    "test-task-handlebars": "NODE_ENV=development node node_modules/gulp/bin/gulp handlebars --serverConfig=/env/config/hapi/config --gulpTaskConfig=/env/config/ --server=gp-boilerplate-webserver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrabarzUndPartner/gp-boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "license": "GNU GPLv3",
  "author": "Grabarz & Partner Werbeagentur GmbH",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@grabarzundpartner.de"
    },
    {
      "name": "Thorn Walli",
      "email": "thorn.walli@grabarzundpartner.de"
    },
    {
      "name": "Boris Horn",
      "email": "boris.horn@grabarzundpartner.de"
    },
    {
      "name": "Jan Szidat",
      "email": "jan.szidat@grabarzundpartner.de"
    }
  ],
  "bugs": {
    "url": "https://github.com/GrabarzUndPartner/gp-boilerplate/issues"
  },
  "homepage": "https://github.com/GrabarzUndPartner/gp-boilerplate#readme",
  "devDependencies": {
    "fb": "^2.0.0",
    "htmlescape": "^1.1.0",
    "jshint": "2.10.2",
    "minimist": "^1.2.0",
    "stylelint": "10.0.1",
    "stylelint-config-css-modules": "1.4.0",
    "stylelint-config-recess-order": "2.0.2",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-processor-html": "1.0.0"
  },
  "dependencies": {
    "ampersand-collection": "^2.0.0",
    "ampersand-model": "^8.0.0",
    "ampersand-state": "^5.0.2",
    "ampersand-view": "^10.0.1",
    "animejs": "^3.0.1",
    "blockadblock": "^3.2.0",
    "classlist-polyfill": "^1.0.3",
    "element-closest": "^3.0.1",
    "element-dataset": "^2.2.4",
    "enum": "^2.2.0",
    "es5-ext": "^0.10.15",
    "es6-promise": "^4.2.6",
    "fg-loadcss": "^2.1.0",
    "fontfaceobserver": "^2.1.0",
    "gp-boilerplate-documentation": "https://github.com/GrabarzUndPartner/gp-boilerplate-documentation.git",
    "gp-boilerplate-environment": "https://github.com/GrabarzUndPartner/gp-boilerplate-environment.git",
    "gp-boilerplate-webserver": "https://github.com/GrabarzUndPartner/gp-boilerplate-webserver.git",
    "gp-module-base": "https://github.com/GrabarzUndPartner/gp-module-base.git",
    "gp-module-history": "https://github.com/GrabarzUndPartner/gp-module-history.git",
    "gp-module-parser": "https://github.com/GrabarzUndPartner/gp-module-parser.git",
    "gp-module-polyfills": "https://github.com/GrabarzUndPartner/gp-module-polyfills.git",
    "gp-module-scroll": "https://github.com/GrabarzUndPartner/gp-module-scroll.git",
    "gp-module-ui-link": "https://github.com/GrabarzUndPartner/gp-module-ui-link.git",
    "gp-module-ui-modal": "https://github.com/GrabarzUndPartner/gp-module-ui-modal.git",
    "gp-module-viewport": "https://github.com/GrabarzUndPartner/gp-module-viewport.git",
    "htmlescape": "^1.1.0",
    "jquery": "^3.1.1",
    "jshint": "^2.8.0",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "modernizr": "^3.4.0",
    "outdated-browser": "^1.0.2",
    "pepjs": "^0.5.2",
    "purecss": "^1.0.0"
  }
}
