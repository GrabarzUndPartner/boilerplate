{
  "name": "clean",
  "task": "./lib/tasks/clean",
  "config": {
    "subtasks": [
      {
        "name": "default",
        "files": {
          "dest": "<%= destination %>/**/*"
        }
      },
      {
        "name": "generated",
        "files": {
          "dest": "<%= destination_root %>/generated/"
        }
      }
    ]
  }
}
